@@once
@@include divtools.try
!html
<script>
  function newPlot(dn, data, layout,config) {
   
   if(typeof dn === 'string') {
      Plotly.newPlot(dn, data, layout, config);
   } else {
      _plot(genID($$.executeDiv), dn, data, layout);
   }
   function _plot(dn, data, layout, config) {
      $$.HTML(`<div id="${dn}" />`);
      $$.lastly(true,() => Plotly.newPlot(dn, data, layout,config));
   }
  }
 </script>
