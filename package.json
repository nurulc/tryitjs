{
  "name": "tryitjs",
  "version": "0.4.7",
  "description": "Generate HTML file with runnable and editable code snippets",
  "main": "index.js",
  "scripts": {
    "tryit": "node bin/gen-tryit.js --src try_src --dest try_it",
    "local": "node bin/gen-tryit.js --src try_src --dest try_it --local",
    "test": "echo \"Error: no test specified\" && exit 1",
    "try": "nodemon --ext \"try css js\"   bin/gen-tryit.js --src try_src --dest try_it --local",
    "build": "babel ref_src -d ref && npm pack && npm install -g .\\tryitjs-0.4.7.tgz && npm run tryit",
    "watch": "nodemon --ext \"js\" --exec \"npm run build\""
  },
  "bin": {
    "tryitjs": "bin/gen-tryit.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nurulc/tryitjs.git"
  },
  "keywords": [
    "generate",
    "html",
    "editable",
    "javascript"
  ],
  "author": "Nurul Choudhury",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nurulc/tryitjs/issues"
  },
  "homepage": "https://github.com/nurulc/tryitjs#readme",
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.4",
    "@babel/polyfill": "^7.10.4",
    "@babel/preset-env": "^7.11.0",
    "@babel/traverse": "^7.11.0",
    "@types/babel__core": "^7.1.9",
    "babel-plugin-loop-optimizer": "^1.4.1",
    "babel-plugin-minify-dead-code-elimination": "^0.5.1",
    "babel-preset-minify": "^0.2.0",
    "glob": "^7.1.6",
    "nodemon": "^2.0.4",
    "reload": "^3.1.0"
  },
  "dependencies": {
    "memoize": "^0.1.1",
    "run-all": "^1.0.1",
    "showdown": "^1.9.1"
  }
}
